{
  "name": "express-ts-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for TS-Express projects",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "preserve": "yarn run build",
    "serve": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q src/index.ts\"",
    "dev": "nodemon",
    "prestart": "yarn run build",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanchew3/express-ts-boilerplate.git"
  },
  "keywords": [
    "boilerplate"
  ],
  "author": "Cassie Chew",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryanchew3/express-ts-boilerplate/issues"
  },
  "homepage": "https://github.com/ryanchew3/express-ts-boilerplate#readme",
  "dependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.6.1",
    "@types/uuid": "^8.3.1",
    "apollo-server-core": "^3.3.0",
    "apollo-server-express": "^3.3.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "class-validator": "^0.13.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.37",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12"
  }
}
